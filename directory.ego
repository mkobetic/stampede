<%! func DirectoryPage(w io.Writer, dir *MailDirectory) error %>

<html>
<nav>
	<ul>
		<% for n, d := range dir.Directories { %>
			<li><a href="<%= d.UrlPath() %>"><%= n %></a></li>
	    <% } %>
		<% for n, f := range dir.Folders { %>
			<li><a href="<%= f.UrlPath() %>"><%= n %></a></li>
	    <% } %>
	</ul>
</nav>
</html>
