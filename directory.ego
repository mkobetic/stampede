<%! func DirectoryPage(w io.Writer, dir *MailDirectory) error %>

<html>
<nav>
	<ul>
		<% for n := range dir.Directories { %>
			<li><a href="/directory/<%= n %>"><%= n %></a></li>
	    <% } %>
		<% for n := range dir.Folders { %>
			<li><a href="/folder/<%= n %>"><%= n %></a></li>
	    <% } %>
	</ul>
</nav>
</html>
