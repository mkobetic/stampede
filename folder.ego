<%! func FolderPage(w io.Writer, folder *MailFolder) error %>

<html>
<nav>
	<ul>
		<% for _, m := range folder.Messages { %>
			<li><a href="<%= m.UrlPath() %>">
				<span><%= m.hSent() %></span>
				<span><%= m.hSubject() %></span>
				<span><%= m.hSender() %></span>
			</a></li>
	    <% } %>
	</ul>
</nav>
</html>
