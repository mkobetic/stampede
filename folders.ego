<%! func FolderTree(w io.Writer, dir *MailDirectory) error %>
<ul>
	<% for _, d := range dir.Directories { %>
		<li><a href="<%= d.UrlPath() %>"><%= d.Label() %></a><% FolderTree(w,d) %></li>
    <% } %>
	<% for _, f := range dir.Folders { %>
		<li class="<%= f.cClass() %>"><a href="<%= f.UrlPath() %>"><%= f.Label() %></a></li>
    <% } %>
</ul>
