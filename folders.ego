<%! func FolderTree(w io.Writer, dir *MailDirectory) error %>
<ul>
	<% for n, d := range dir.Directories { %>
		<li><a href="<%= d.UrlPath() %>"><%= n %></a><% FolderTree(w,d) %></li>
    <% } %>
	<% for n, f := range dir.Folders { %>
		<li><a href="<%= f.UrlPath() %>"><%= n %></a></li>
    <% } %>
</ul>
